{
    "python.pythonPath": "/usr/bin/python3",
    "latex-workshop.view.pdf.viewer": "external",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path.mac": "/Users/barun/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.minikube-path.mac": "/Users/barun/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.draft-path.mac": "/Users/barun/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vscode-kubernetes.minikube-path.mac": "/Users/barun/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "workbench.colorTheme": "Monokai",
    "redhat.telemetry.enabled": true,
    "go.toolsManagement.autoUpdate": true,
    "git.ignoreMissingGitWarning": true,
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindings": [
    ],
    "vim.normalModeKeyBindingsNonRecursive": [

        {
            "before": ["J"],
            "after": ["5", "j"]
        },
        {
            "before": ["K"],
            "after": ["5", "k"]
        },
        {
            "before": ["<leader>", "w"],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": ["<C-d>", "z", "z"],
            "after": ["<C-d>"]
        },
        {
            "before": ["<C-u>", "z", "z"],
            "after": ["<C-u>"]
        },
        {
            "before": ["d", "i", "S"],
            "after": ["d", "i", "["]
        },
        {
            "before": ["c", "c"],
            "after": ["~"],
        }
    ],
    "vim.commandLineModeKeyBindings": [
    ],
    "editor.lineNumbers": "relative",
    "vim.commandLineModeKeyBindingsNonRecursive": [
    ]
}